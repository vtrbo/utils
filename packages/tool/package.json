      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "pnpm clean:dist && pnpm clean:deps",
    "clean:dist": "rimraf dist",
    "clean:deps": "rimraf node_modules"
  }
}
