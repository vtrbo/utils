{
  "name": "@vtrbo/utils",
  "version": "0.0.12",
  "packageManager": "pnpm@7.8.0",
  "description": "Collection of common JavaScript or TypeScript utils.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "keywords": [
    "utils",
    "tools"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "require": "./dist/array.js",
      "import": "./dist/array.mjs"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "require": "./dist/color.js",
      "import": "./dist/color.mjs"
    },
    "./fn": {
      "types": "./dist/fn.d.ts",
      "require": "./dist/fn.js",
      "import": "./dist/fn.mjs"
    },
    "./is": {
      "types": "./dist/is.d.ts",
      "require": "./dist/is.js",
      "import": "./dist/is.mjs"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "require": "./dist/string.js",
      "import": "./dist/string.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "esno generate/entry.ts && esno generate/docs.ts",
    "build": "nr generate && tsup",
    "dev": "nr generate && tsup --watch",
    "lint": "eslint .",
    "release": "nr build && bumpp && npm publish --access=public",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "playground:dev": "nr -C playground dev",
    "playground:build": "nr -C playground build",
    "docs:dev": "nr -C docs dev",
    "docs:build": "nr -C docs build",
    "docs:serve": "nr -C docs serve"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@vtrbo/eslint-config": "^0.0.6",
    "@vtrbo/ni": "^0.0.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "pnpm": "^7.8.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.3"
  }
}
