{
  "name": "@vtrbo/utils",
  "type": "module",
  "version": "0.4.0-beta.3",
  "description": "Collection of common JavaScript or TypeScript utils.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo",
  "bugs": "https://github.com/vtrbo/utils/issues",
  "keywords": [
    "typescript",
    "javascript",
    "utils",
    "vue",
    "react",
    "vite"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "index.cjs",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "pnpm -r run build && tsup",
    "release": "pnpm build && pnpm bumpp && pnpm publish",
    "bumpp": "bumpp packages/*/package.json package.json",
    "publish": "pnpm packages/*/package.json package.json publish --no-git-checks --access=public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@vtrbo/utils-log": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.26",
    "@types/node": "^20.8.5",
    "bumpp": "^9.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
